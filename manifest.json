{
    "name": "Line Tab",
    "description" : "A new tab page built for the quickest browsing experience",
    "icons": { 
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png" 
    },
    "version": "0.0.0.15",
    "manifest_version": 2,
    "chrome_url_overrides" : {
      "newtab": "index.html"
