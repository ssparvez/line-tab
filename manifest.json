{
    "name": "lineTab",
    "description" : "A simple new tab page built for developers",
    "icons": { 
      "16": "assets/icon16.png",
      "48": "assets/icon48.png",
      "128": "assets/icon128.png" 
    },
    "version": "0.0.0.1",
    "manifest_version": 2,
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "permissions": [
      "bookmarks",
      "tabs",
      "storage"
    ]
  }